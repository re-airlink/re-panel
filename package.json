{
  "name": "airlink-panel",
  "version": "1.0.0",
  "description": "Airlink is a simple-to-use game server management panel",
  "repository": {
    "type": "git",
    "url": "https://github.com/AirlinkLabs/panel"
  },
  "author": "AirlinkLabs",
  "license": "MIT",
  "scripts": {
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/app.js",
    "start": "npm run serve",
    "build:css": "tailwindcss -i ./public/tw.conf -o ./public/styles.css",
    "watch:css": "tailwindcss -i ./public/tw.conf -o ./public/styles.css --watch",
    "build": "tsc",
    "start:dev": "nodemon",
    "format": "prettier --write src/",
    "migrate:dev": "npx prisma migrate dev --name init && npx prisma generate",
    "migrate:deploy": "npx prisma migrate deploy && npx prisma generate",
    "test": "jest --forceExit",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "prisma": "^5.22.0"
  },
  "devDependencies": {
    "@prisma/client": "^5.22.0",
    "@tailwindcss/forms": "^0.5.9",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/express-ws": "^3.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.6",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.5.13",
    "express-ws": "^5.0.2",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "ws": "^8.18.0"
  }
}
